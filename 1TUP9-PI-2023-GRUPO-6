SubProceso precios(BsAsBarilo)// este sub esta hecho para los calculos de los precios, hay que ver si es necesario hacer unna funcion por cada vuelo o no. me parece mas conveniente usar funciones en estos casos
	si cantBB <= 20 Entonces
		
		si bsasBarilo3[4,i] = "verdadero"
			bsasBarilo[5,i] = // precio 150.000 + 5% 	
		sino 
			bsasBarilo[5,i] = // precio 150.000 
		FinSi                                                //repetir estas operaciones para cada ruta de vuelo
	SiNo
		si cantBB > 20 y cantBB <= 60                         // buscar como hacer para que esto asigne ese precio para cada pasajero 
			si bsasBarilo3[4,i] = "verdadero"
				bsasBarilo[5,i] = // precio 150.000 +10% + 5% 	
			sino 
				bsasBarilo[5,i] = // precio 150.000 + 10%
			FinSi
		SiNo
			si cantBB > 60 y cantBB <= 120
				si bsasBarilo3[4,i] = "verdadero"
					bsasBarilo[5,i] = // precio 180.000 + 5% 	
				sino 
					bsasBarilo[5,i] = // precio 180.000 
				FinSi
			FinSi
		FinSi
	FinSi
	
	
FinSubProceso


Funcion Pasaj <- Pasajeros(lista)
	Definir NumPasaj, arrayPasaj, Salir Como Entero
	
	Para i<-0 Hasta 5
		Repetir
			
			NumPasaj = i
			Dimension arrayPasaj[NumPasaj]
			Dimension Ruta[NumPasaj]
			Dimension Telefono[NumPasaj]
			Dimension Dni[NumPasaj]
			Dimension Ruta[NumPasaj]
			Dimension IDPasajero[NumPasaj]
			Dimension Asiento[NumPasaj]
			Dimension CostoPasaj[NumPasaj]
			
			
		Mientras Que (Salir = 0)
	FinPara
FinFuncionAlgoritmo sin_titulo
	
	definir opc, ruta, DNI, tel, rutaOrd Como Entero
	Definir nombre como cadena
	Definir equipaje, NumAsi Como Entero
	definir precios Como Real
	definir opc2 Como caracter
